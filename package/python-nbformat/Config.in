config BR2_PACKAGE_PYTHON_NBFORMAT
	bool "python-nbformat"
	select BR2_PACKAGE_PYTHON_IPYTHON_GENUTILS # runtime
	select BR2_PACKAGE_PYTHON_TRAITLETS # runtime
	select BR2_PACKAGE_PYTHON_JSONSCHEMA # runtime
	select BR2_PACKAGE_PYTHON_JUPYTER_CORE # runtime
	help
	  The Jupyter Notebook format.

	  http://jupyter.org

config BR2_PACKAGE_PYTHON_NBFORMAT_TESTS
	bool "nbformat.tests"
	help
		Include test files.
		Selecting this option adds about 0.7 MB to the target file
		system.
